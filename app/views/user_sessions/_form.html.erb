<%= form_with url: user_sessions_path, method: :post do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <br><br>
  <div class="actions">
    <%= f.submit "Login", class:"btn btn-primary btn-block" %>
  </div>
<% end %>
